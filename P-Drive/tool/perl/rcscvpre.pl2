foreach $ifname (@ARGV) {
	if("$ifname" ne "*.*") {
		$ofname = $ifname;
		$ofname =~ tr/A-Z/a-z/;
		if ('v' ne chop( $ofname )) {
			print "\n$ifname";
		}
	}
}
